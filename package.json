{
  "name": "shining-star-monorepo",
  "private": true,
  "workspaces": ["client", "server"],
  "scripts": {
    "dev:client": "npm --prefix client run dev",
    "dev:server": "npm --prefix server run dev",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
